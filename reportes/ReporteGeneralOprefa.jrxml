	<parameter name="Tipo" class="java.lang.String"/>
	<parameter name="Año" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT 
	Tipo =  $P{Tipo} ,
	Año,
	p.DNI,
    p.A_Paterno + ' ' + p.A_Materno + ' ' + p.Nombres AS Empleado,
    ISNULL([APORTE], 0) AS Aporte,
    ISNULL([DEUDA], 0) AS Deuda
FROM 
    (
        SELECT 
            h.Documento,
            CASE 
                WHEN h.Estado IN (1, 4, 5) THEN 'APORTE'
                WHEN h.Estado IN (2, 3) THEN 'DEUDA'
                ELSE 'Otro'
            END AS Tipo,
            h.Monto,
            h.Año
        FROM [dbo].[HistorialOprefa] h
        INNER JOIN [dbo].[Personal] p ON h.Documento = p.Dni
        WHERE h.Año =  $P{Año} 
    ) AS DatosClasificados
PIVOT
    (
        SUM(Monto)
        FOR Tipo IN ([APORTE], [DEUDA])
    ) AS pvt
INNER JOIN [dbo].[Personal] p ON p.Dni = pvt.Documento
ORDER BY 
    p.A_Paterno, p.A_Materno, p.Nombres;]]>
	</queryString>
